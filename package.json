{
  "name": "scan-story",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "check-types": "turbo run check-types",
    "generate-asset-paths": "npx tsx scripts/generateAssetPaths.ts",
    "full-build": "npx tsx scripts/generateAssetPaths.ts && turbo run build"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/react": "^18.3.24",
    "fs-extra": "^11.3.0",
    "globby": "^14.1.0",
    "modern-css-reset": "^1.4.0",
    "react": "^18.3.1"
  }
}
